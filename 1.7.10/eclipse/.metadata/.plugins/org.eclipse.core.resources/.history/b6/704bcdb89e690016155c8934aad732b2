package com.pikachu923.BananaCraft.init;

import com.pikachu923.BananaCraft.Entity.EntityAbe;

import cpw.mods.fml.common.registry.EntityRegistry;
import net.minecraft.entity.EnumCreatureType;

public class EntityInit {
	public static final Class KualaClass = com.pikachu923.BananaCraft.Entity.EntityAbe.class;
public static void Init () {
	Other.registerEntity(KualaClass, "Kuala", 8421504, 0, true);
	EntityRegistry.addSpawn(KualaClass, 10, 1, 5, EnumCreatureType.creature);
	}
}
